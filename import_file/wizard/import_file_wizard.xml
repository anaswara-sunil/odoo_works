<?xml version="1.0" encoding="utf-8" ?>
<odoo>
   <record id="import_file_wizard_form" model="ir.ui.view">
       <field name="name">import.file.wizard.form</field>
       <field name="model">import.file.wizard</field>
       <field name="arch" type="xml">
           <form string="Import Lots and Serial Number">
               <group>
                   <group>
                       <field name="file"/>
                   </group>
               </group>
               <footer>
                   <button name="action_import_file" string="Import" type="object" class="btn-primary"/>
               </footer>
           </form>
       </field>
   </record>
</odoo>
